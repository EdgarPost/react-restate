		"@types/jest": "^26.0.20",
		"@types/shallowequal": "^1.1.1",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"babel-eslint": "^10.0.2",
		"babel-jest": "^26.6.3",
		"eslint": "^7.19.0",
